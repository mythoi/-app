<resources>
    <!-- Base application theme. -->
    <style name="AppTheme4" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary4</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark4</item>
        <item name="colorAccent">@color/colorAccent4</item>
		<item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>

	</style>

    <style name="AppTheme4.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>



    <style name="AppTheme4.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="AppTheme4.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />




</resources>
