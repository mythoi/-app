<resources>
    <!-- Base application theme. -->
    <style name="AppTheme7" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary7</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark7</item>
        <item name="colorAccent">@color/colorAccent7</item>
		<item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>

	</style>

    <style name="AppTheme7.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>



    <style name="AppTheme7.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="AppTheme7.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />




</resources>
